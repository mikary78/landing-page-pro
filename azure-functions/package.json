{
  "name": "landing-page-pro-functions",
  "version": "1.0.0",
  "description": "Azure Functions for Landing Page Pro",
  "main": "dist/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "shx cp host.json dist/ && shx cp package.dist.json dist/package.json",
