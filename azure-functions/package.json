    "@azure/functions": "^4.5.0",
    "@google/generative-ai": "^0.1.3",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "azure-functions-core-tools": "^4.0.5455",
    "shx": "^0.4.0"
  }
}
